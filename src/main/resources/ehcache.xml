<ehcache>
    <diskStore path="java.io.tmpdir"/>

    <cache
            statistics="true"
            name="decoded_names"
            maxElementsInMemory="120"
            eternal="true"
            overflowToDisk="false"
            diskPersistent="true"
            memoryStoreEvictionPolicy="LFU"/>
    <cache
            statistics="true"
            name="encoded_names"
            maxElementsInMemory="120"
            eternal="true"
            overflowToDisk="false"
            diskPersistent="true"
            memoryStoreEvictionPolicy="LFU"/>


</ehcache>
